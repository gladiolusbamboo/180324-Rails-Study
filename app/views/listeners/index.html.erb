<p id="notice"><%= notice %></p>

<h1>Listeners</h1>

<table>
  <thead>
    <tr>
      <th>Listenername</th>
      <th>Password digest</th>
      <th>Email</th>
      <th>Is male</th>
      <th>Roles</th>
      <th>Reviews count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listeners.each do |listener| %>
      <tr>
        <td><%= listener.listenername %></td>
        <td><%= listener.password_digest %></td>
        <td><%= listener.email %></td>
        <td><%= listener.is_male %></td>
        <td><%= listener.roles %></td>
        <td><%= listener.reviews_count %></td>
        <td><%= link_to 'Show', listener %></td>
        <td><%= link_to 'Edit', edit_listener_path(listener) %></td>
        <td><%= link_to 'Destroy', listener, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listener', new_listener_path %>
